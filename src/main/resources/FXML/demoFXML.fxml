      <Button layoutX="625.0" layoutY="297.0" mnemonicParsing="false" onAction="#right" text="RIGHT"/>
      <ImageView fitHeight="439.0" fitWidth="512.0" layoutX="118.0" layoutY="77.0" pickOnBounds="true"
