<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="594.0" prefWidth="704.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.DemoFXML">
   <children>
      <Button layoutX="358.0" layoutY="25.0" mnemonicParsing="false" onAction="#up" text="UP"/>
